<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$$if(dir)$ dir="$dir$"$endif$>
<head>
  <meta charset="utf-8">
  <meta name="generator" content="pandoc">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(keywords)$
  <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ – $endif$$pagetitle$$if(backtoindex)$ &mdash; Introduction to Numerical Analysis 1$endif$</title>
  <style type="text/css">code{white-space: pre;}</style>
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
<style>
    .markdown-body {
        box-sizing: border-box;
        min-width: 200px;
        max-width: 980px;
        margin: 0 auto;
        padding: 45px;
    }
    h2.author {
        font-size: 12pt;
        border: 0px;
    }
	footer {
		margin-top: 32px;
		padding: 1em;
		border-top: 1px dashed SkyBlue;
		text-align: center;
		font-size: 70%;
		color: SkyBlue;
	}
    /* fix for fractions not having a margin */
    .mjx-mfrac {
        margin-right: 0.2em;
    }
    @media print {
        .markdown-body {
            font-size: 9pt;
            line-height: 1.1;
        }
        #back-to-index {
            display: none;
        }
    }
</style>
<script type="text/x-mathjax-config">
MathJax.Hub.Config({
  TeX: { equationNumbers: { autoNumber: "AMS" } }
});
</script>
$if(math)$
  $math$
$endif$
  <!--[if lt IE 9]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv-printshiv.min.js"></script>
  <![endif]-->
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
<article class="markdown-body">
$if(backtoindex)$
<p>
<a href="index.html" id="back-to-index">↜ Back to index</a>
<span style="float:right; font-style:italic">Introduction to Numerical Analysis 1</span>
</p>
$endif$
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
<h1 class="title">$title$</h1>
$if(subtitle)$
<h1 class="subtitle">$subtitle$</h1>
$endif$
$for(author)$
<h2 class="author">$author$</h2>
$endfor$
$if(date)$
<h3 class="date">$date$</h3>
$endif$
</header>
$endif$
$if(toc)$
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
$body$
$for(include-after)$
$include-after$
$endfor$

<footer>
<p>Made with <a href="http://www.vim.org/">Vim</a> and <a href="http://johnmacfarlane.net/pandoc/">pandoc</a>.</p> <p>Math delivered using <a href="http://www.mathjax.org/">MathJax</a>.</p>
<p>Norbert Požár, 2016</p>
</footer>
</article>
</body>
</html>
